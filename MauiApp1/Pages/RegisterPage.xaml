<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.Pages.RegisterPage"
             Title="Тут надо регаться">
    <VerticalStackLayout>
        <Label Text="Логин"/>
        <Entry x:Name="LoginEntry"/>
        <Label Text="Почта"/>
        <Entry x:Name="Mail"/>
        <Label Text="Пароль"/>
        <Entry x:Name="PasswordEntry"/>
        <Label Text="Повторите пароль"/>
        <Entry x:Name="SecondPasswordEntry"/>
        <Button Text="Зарегистрироваться" Clicked="RegButton"/>
        <Button Text="Войти" Clicked="SignButton"/>
      
    </VerticalStackLayout>
</ContentPage>